<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Toplevel" id="toplevel2">
    <property name="height">300</property>
    <property name="title" translatable="yes">Player Stats Tool</property>
    <property name="width">300</property>
    <child>
      <object class="ttk.Frame" id="frame6">
        <property name="height">300</property>
        <property name="width">300</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
        <child>
          <object class="ttk.Button" id="button1">
            <property name="command" type="command" cbtype="simple">button1_clicked</property>
            <property name="text" translatable="yes">Search</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.61</property>
              <property name="rely">0.17</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="textEntry1">
            <property name="exportselection">true</property>
            <property name="font">TkDefaultFont</property>
            <property name="justify">left</property>
            <property name="state">normal</property>
            <property name="takefocus">false</property>
            <property name="textvariable">string:enteredPlayer</property>
            <property name="validate">focusin</property>
            <property name="width">15</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.1</property>
              <property name="rely">0.19</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label2">
            <property name="text" translatable="yes">Player Name:</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.1</property>
              <property name="rely">0.07</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Text" id="statsDisplay">
            <property name="blockcursor">false</property>
            <property name="height">4</property>
            <property name="setgrid">false</property>
            <property name="text" translatable="yes">Kills:
Deaths:
Wins:
KD Ratio: </property>
            <property name="width">20</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.1</property>
              <property name="rely">0.35</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button2">
            <property name="command" type="command" cbtype="simple">button2_clicked</property>
            <property name="text" translatable="yes">Clear Search</property>
            <property name="width">26</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.1</property>
              <property name="rely">0.74</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
